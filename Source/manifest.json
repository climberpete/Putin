{
        "manifest_version": 2,
        "name": "Putinizer",
        "version": "0.1",
        "description": "Replaces images on a webpage with pictures of Vladimir Putin",
        "content_scripts": 
        [
                {
                        "matches": ["*://*/*"],
                        "js": ["jquery-1.11.0.min.js", "content_script.js"],
                        "run_at": "document_end"
                }
        ]
}
